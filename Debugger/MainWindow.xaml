<mui:ModernWindow x:Class="Debugger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="asDevelop" Style="{StaticResource MyWindow}" Height="350" Width="525" IsTitleVisible="True">
    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="Develop" Source="/IDE/IDEView.xaml" />
        <mui:Link DisplayName="Debug" Source="/Screens/DebugScreen.xaml" />
        <mui:Link DisplayName="Plugins" Source="cmd://showPlugins"/>
    </mui:ModernWindow.TitleLinks>
</mui:ModernWindow>
